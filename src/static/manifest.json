{
    "manifest_version": 3,
    "name": "Price Tracker RW",
    "description": "Suggests best price deals for you",
    "version": "1.0.0",
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "action": {
        "default_title": "Click me",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "bookmarks",
        "unlimitedStorage"
    ],
    "background": {
        "service_worker": "background.js"
      },
    "content_scripts": [{
        "matches": ["https://*.google.com/*"],
        "js": ["contentScript.js"]
    }
    ]
}